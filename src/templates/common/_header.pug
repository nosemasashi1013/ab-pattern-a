      include ./_accessibility.pug

      .content-search.display-none
        .content-body
          .content-form
            form(action="https://www.pref.fukui.lg.jp/search.html" class="search-box form-header")
              input(
                type="text"
                class="text search-box-header"
                id="q_header"
                name="q"
                placeholder="検索"
              )
              button(
                accesskey="s"
                class="button-search button-search-header"
                type="submit"
                onClick="gtag('event', 'click', {'event_category': 'search-free-word', 'event_label': 'header'});"
              )
                i(class="fas fa-search")
          .icon-menu-icon.x-search-menu-trigger
            .menu-icon
              i(class="fas fa-times close-icon")
            .menu-text
              | 閉じる
      .icon-menu-icon.x-global-mune-trigger
        .menu-icon.menu-open
          img(src=pathImage + "common/menu_icon.png" alt="メニュー" class="menu-img")
        .menu-icon.menu-close
          i(class="fas fa-times close-icon")
        .menu-text
          | メニュー

    .conten-bottom
      - var additionalValue  = "-sp"
      include ./_accessibility.pug
      nav.nav
        ul.nav-list
          each item in globalNavi
            li.nav-item 
              a(
                href=item.link
                onClick="gtag('event', 'click', {'event_category': 'category', 'event_label': 'header', 'value': '"+ item.text + "'});"
              ) 
                h1 
                  =item.text
                i(class="fas fa-angle-right")
    .content-search-box.box-shadow
      include ../parts/_search-keyword-list.pug
      +featureKeywords('header')


