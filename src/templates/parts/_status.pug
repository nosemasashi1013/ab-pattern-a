.search-menu-container.status-container.layout-default
  h2.status-title
    | あなたの状況
  ul.status-list 
    each item in status 
      li.status-item.box-shadow
        label(
          class="checkbox"
          for="status-" + item.value
        )
          input(
            id="status-" + item.value
            class="checkbox-input visually-hidden"
            type="checkbox"
            name="status"
            value=item.value
            checked=item.checked
            onClick="gtag('event', 'click', {'event_category': 'status', 'event_label': '"+item.value+"'});"
          )
          span.checkbox-label
            =item.label

